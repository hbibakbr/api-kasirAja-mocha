{
  "name": "api-mocha-kasiraja",
  "version": "1.0.0",
  "description": "api automation kasirAja using supertest",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npx mocha",
    "suites": "npx mocha test/suites/*.js",
    "suites:user": "npx mocha test/suites/user.suite.js",
    "suites:unit": "npx mocha test/suites/unit.suite.js",
    "suites:category": "npx mocha test/suites/category.suite.js",
    "suites:customer": "npx mocha test/suites/customer.suite.js",
    "suites:product": "npx mocha test/suites/product.suite.js",
    "suites:report": "npx mocha --reporter mochawesome test/suites/*.js"
  },
  "keywords": [
    "api"
  ],
  "author": "habib",
  "license": "ISC",
  "dependencies": {
    "api-mocha-kasiraja": "file:",
    "chai": "^5.1.2",
    "jsonschema": "^1.4.1",
    "supertest": "^7.0.0"
  },
  "mocha": {
    "timeout": "10000"
  },
  "devDependencies": {
    "mocha": "^10.8.2",
    "mochawesome": "^7.1.3"
  }
}
